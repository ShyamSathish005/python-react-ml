{
  "name": "python-react-ml",
  "version": "1.0.1",
  "description": "Run Python ML models directly in React and React Native apps - no backend required",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "python"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "dependencies": {
    "pyodide": "^0.24.0",
    "jszip": "^3.10.0"
  },
  "devDependencies": {
    "tsup": "^7.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "keywords": [
    "python",
    "pyodide", 
    "machine-learning",
    "webassembly"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ShyamSathish005/python-react-ml.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/ShyamSathish005/python-react-ml#readme",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}